<?xml version="1.0" encoding="UTF-8"?>
<ConnectedApp xmlns="http://soap.sforce.com/2006/04/metadata">
    <contactEmail>it_support@intapp.com</contactEmail>
    <iconUrl>https://login.salesforce.com/logos/Apps/Confluence/icon.png</iconUrl>
    <label>Intapp Wiki</label>
    <logoUrl>https://login.salesforce.com/logos/Apps/Confluence/logo.png</logoUrl>
    <samlConfig>
        <acsUrl>https://www.intapp.com:443/wiki/plugins/servlet/saml/auth</acsUrl>
        <encryptionType>AES_128</encryptionType>
        <entityUrl>intapp.wiki</entityUrl>
        <issuer>https://intapp.my.salesforce.com</issuer>
        <samlNameIdFormat>Persistent</samlNameIdFormat>
        <samlSigningAlgoType>SHA1</samlSigningAlgoType>
        <samlSubjectCustomAttr>Wiki_Username</samlSubjectCustomAttr>
        <samlSubjectType>CustomAttribute</samlSubjectType>
    </samlConfig>
    <startUrl>https://www.intapp.com/wiki/</startUrl>
</ConnectedApp>
