<apex:page standardController="Account">
<h1>JIRA issues associated to the Account Number of <b>{!account.intapp_account_number__c} </b></h1>
<br/><br/>

<input type="hidden" id="id" value="{!account.ID}"/>

<script>
var counter = 0;
function searchUrl() {
if(counter == 0) {
var id18 = document.getElementById('id').value;
var id15 = id18.substring(0,15);
document.getElementById('jiraSearch').src="https://www.intapp.com/jira/plugins/servlet/crmSearch?salesforce-account=" + id15;

counter = 1;
}
}
</script>

<iframe id="jiraSearch" onLoad="searchUrl();" src="" scrolling="yes" height="600px" width="100%" frameborder="0"></iframe>
</apex:page>