<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Gives users access to read/write the Forecast Notes field on the Project object and read/write access on the High Confidence Upside on the opportunity</description>
    <fieldPermissions>
        <editable>true</editable>
        <field>Opportunity.Forecast_Notes__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Opportunity.High_Confidence_Upside__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Access Forecast Notes Field</label>
    <license>Salesforce</license>
</PermissionSet>
