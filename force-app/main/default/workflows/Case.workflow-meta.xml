        <actions>
            <name>Case_Estimated_Completion_Date_Changed</name>
            <type>Alert</type>
        </actions>
        <active>false</active>
        <description>Rule Code: CASE-038Exxx
Case Est. Completion Date Changed
Monitors when the Estimated Completion Date field changes on a Professional Services case.  Sends the email alert.</description>
        <formula>AND (
 IsClosed = FALSE,
 ISCHANGED(PSCase_Project_Exp_Compl_Date__c),
 NOT(ISNEW()),
 BEGINS(Record_Type_Name__c, &quot;Implementation&quot;),
 NOT( Services_Revenue_Total__c  = 0 ),
 NOT( AND( YEAR( PSCase_Project_Exp_Compl_Date__c ) = YEAR( Project_Exp_Compl_Date_Prior__c ), MONTH( PSCase_Project_Exp_Compl_Date__c ) = MONTH( Project_Exp_Compl_Date_Prior__c ) ) )
)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
