    <rules>
        <fullName>Rule Code%3A CASE-018xxOx</fullName>
        <actions>
            <name>SendCaseToDTE</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>Rule Code: CASE-018xxOx
UpdateCaseInDTE
Sends out an outbound message to update Case in DTE</description>
        <formula>ISNEW() || (
(ISCHANGED(AccountId) ||
ISCHANGED(Date_Closed__c) ||
ISCHANGED(IsClosed) ||
ISCHANGED(RecordTypeId) ||
ISCHANGED(Status) ||
ISCHANGED(Subject)) &amp;&amp;
(Record_Type_Name__c &lt;&gt; &apos;Support Survey&apos; &amp;&amp;
Record_Type_Name__c &lt;&gt; &apos;Survey&apos; &amp;&amp;
Record_Type_Name__c &lt;&gt; &apos;FRU&apos; &amp;&amp;
Record_Type_Name__c &lt;&gt; &apos;Procurement&apos; &amp;&amp;
LEFT(Record_Type_Name__c, 14) &lt;&gt; &apos;Implementation&apos;))</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
