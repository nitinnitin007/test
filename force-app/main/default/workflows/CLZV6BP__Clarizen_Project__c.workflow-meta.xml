    <rules>
        <fullName>Rule Code%3A CP-001xxOx</fullName>
        <actions>
            <name>SendClarizenProjectToDTE</name>
            <type>OutboundMessage</type>
        </actions>
        <active>true</active>
        <description>Rule Code: CP-001xxOx
UpdateClarizenProjectInDTE
Sends an outbound message to DTE to update clz Project in it</description>
        <formula>ISNEW() ||
ISCHANGED(CLZV6BP__CLZ_DueDate__c) || 
ISCHANGED(CLZV6BP__CLZ_StartDate__c) || 
ISCHANGED(CLZV6BP__ParentProject__c) || 
ISCHANGED(Go_Live_Date__c) ||
ISCHANGED(Project_Phase__c) ||
ISCHANGED(Project_Status__c) ||
ISCHANGED(Project_Status_Notes__c) ||
ISCHANGED(Internal_to_Intapp_Notes__c) ||
ISCHANGED (CLZV6BP__CLZ_ClosingNotes__c)||
ISCHANGED(ProjectKickoffDate__c)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
