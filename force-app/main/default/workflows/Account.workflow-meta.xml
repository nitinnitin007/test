    <rules>
        <fullName>Rule Code%3A ACT-001Exxx</fullName>
        <actions>
            <name>ProductUseCasesUpdated</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Account.Product_Usage__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
