<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Completeness_Score_Business_Case__c</fullName>
    <externalId>false</externalId>
    <formula>(IF(
  ISBLANK(Contract_Signer__c),
  0,
  1
)+
IF(
  ISBLANK(TEXT(Delivered_Business_Case_ROI__c)),
  0,
  1
)+
IF(
  Business_Case_Attached_to_Opportunity__c,
  1,
  0
))/3</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Completeness Score: Business Case</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
