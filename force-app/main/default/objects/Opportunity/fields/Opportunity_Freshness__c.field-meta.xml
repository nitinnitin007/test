<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Opportunity_Freshness__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  AND(
    Probability&lt;&gt;0,
    Probability&lt;&gt;100
  ),
  IF(
    TODAY()-Last_Stage_Change_Date__c &lt;120,
    &quot;Fresh&quot;,
    &quot;Stale&quot;
  ),
  &quot;Closed or Target&quot;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Opportunity Freshness</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
