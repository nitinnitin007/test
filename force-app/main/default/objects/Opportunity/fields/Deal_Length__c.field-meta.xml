<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Deal_Length__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  IsClosed,
  IF(
    CloseDate-First_Entered_Pre_Shopping_Date__c&lt;0,
    0,
    CloseDate-First_Entered_Pre_Shopping_Date__c
  ),
  IF(
    TODAY()-First_Entered_Pre_Shopping_Date__c&lt;0,
    0,
    TODAY()-First_Entered_Pre_Shopping_Date__c
  )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
