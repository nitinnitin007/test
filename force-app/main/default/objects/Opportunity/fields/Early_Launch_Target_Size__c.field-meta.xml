<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Early_Launch_Target_Size__c</fullName>
    <description>What size range does this firm fall into?</description>
    <externalId>false</externalId>
    <formula>IF (Account.Lawyers__c &gt; 249, &quot;Too Big (&gt;250)&quot;,
