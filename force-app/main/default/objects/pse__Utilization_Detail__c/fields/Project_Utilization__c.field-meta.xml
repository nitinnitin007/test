<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_Utilization__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  NOT(ISBLANK(pse__Historical_Start_Date__c)),
  IF(
    pse__Historical_Calendar_Hours__c=0,
    1, 
    (pse__Historical_Billable_Hours__c+pse__Historical_Non_Billable_Hours__c)/pse__Historical_Calendar_Hours__c 
  ),
  IF(
    pse__Scheduled_Calendar_Hours__c=0,
    1, 
    (pse__Scheduled_Billable_Hours__c+pse__Scheduled_Non_Billable_Hours__c)/pse__Scheduled_Calendar_Hours__c 
  )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Project Utilization</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
