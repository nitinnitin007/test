<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ACV__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
AND(MOD(SBQQ__Quote__r.SBQQ__SubscriptionTerm__c, 12) != 0,License_Product__c=TRUE),
SBQQ__PackageTotal__c*(12/SBQQ__Quote__r.SBQQ__SubscriptionTerm__c),
IF(Bundle_Product__c=TRUE,Bundle_ACV__c,
SBQQ__PackageTotal__c))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ACV</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
