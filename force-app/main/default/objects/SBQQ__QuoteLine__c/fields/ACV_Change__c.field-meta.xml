<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ACV_Change__c</fullName>
    <description>BIZ-50365, If ACV Change Manual IS Blank THEN Original ACV Change ELSE ACV Change Manual</description>
    <externalId>false</externalId>
    <formula>IF(OR( ACV_Change_Manual__c &gt;0,ACV_Change_Manual__c&lt;0), ACV_Change_Manual__c,  Original_ACV_Change__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>ACV Change</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
