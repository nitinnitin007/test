<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Segment_Months__c</fullName>
    <description>Calculate the difference of months between Start Date and End Date, created as per BIZ-46251</description>
    <externalId>false</externalId>
    <formula>IF(DAY(SBQQ__EffectiveEndDate__c) &lt; DAY(SBQQ__EffectiveStartDate__c), 
		        (MONTH(SBQQ__EffectiveEndDate__c)) + (12*(YEAR(SBQQ__EffectiveEndDate__c)-(YEAR(SBQQ__EffectiveStartDate__c)))) - 
			        (MONTH(SBQQ__EffectiveStartDate__c)), 
		        (MONTH(SBQQ__EffectiveEndDate__c)) + (12*(YEAR(SBQQ__EffectiveEndDate__c)-YEAR(SBQQ__EffectiveStartDate__c))) - 
			        MONTH(SBQQ__EffectiveStartDate__c) + 1
	       )</formula>
    <label>Segment Months</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
