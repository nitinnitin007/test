<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>OSA_list_price_for_bundle_is_blocked</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
 Is_Bundle_Product__c = TRUE,
ISCHANGED(OSA_List_Price__c)
)</errorConditionFormula>
    <errorMessage>You cant change OSA List price on a Bundle level. Please update child products price.</errorMessage>
</ValidationRule>
