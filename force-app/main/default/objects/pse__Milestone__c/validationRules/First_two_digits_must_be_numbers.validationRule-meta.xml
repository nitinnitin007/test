<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_two_digits_must_be_numbers</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(
   ISNUMBER(LEFT(Name,2))
)</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>The first two digits of a Milestone must be a number in order to show the sequence of milestones.</errorMessage>
</ValidationRule>
