<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hours_Delta_Percent__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  AND(
    NOT(ISBLANK(pse__Planned_Hours__c)),
    NOT(pse__Planned_Hours__c = 0)
  ),
  (Total_Hours__c - pse__Planned_Hours__c)/pse__Planned_Hours__c,
  NULL
)</formula>
    <label>Actual Hours Delta (%)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
