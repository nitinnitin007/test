<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>refedge__Root_level_Attribute_Reassigned</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(refedge__Parent__c)), refedge__Level__c = 0)</errorConditionFormula>
    <errorMessage>The Root level Attribute cannot be made child of another Attribute.</errorMessage>
</ValidationRule>
