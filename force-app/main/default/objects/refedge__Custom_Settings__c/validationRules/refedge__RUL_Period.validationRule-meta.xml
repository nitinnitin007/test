<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>refedge__RUL_Period</fullName>
    <active>true</active>
    <errorConditionFormula>(refedge__RUL_Period__c  &gt; 365) ||(refedge__RUL_Period__c  &lt; 1)</errorConditionFormula>
    <errorDisplayField>refedge__RUL_Period__c</errorDisplayField>
    <errorMessage>You must specify a number of days in the period from 1 to 365</errorMessage>
</ValidationRule>
