<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Fillout_RCA_and_Solution</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR(ISBLANK(RCA__c),ISBLANK(Solution__c)), ISPICKVAL(Status,&quot;Closed - Resolved&quot;), Record_Type_Name__c =&quot;Support&quot;)</errorConditionFormula>
    <errorMessage>Please fill out RCA and Solution</errorMessage>
</ValidationRule>
