<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Hour_Created_PST__c</fullName>
    <description>This formula calculates the hour in which a case was created in PST. Only to be used in reports - not to be displayed on actual case.</description>
    <externalId>false</externalId>
    <formula>VALUE( MID( TEXT( CreatedDate - 0.2919 ) , 12, 2) )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Hour Created PST</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
