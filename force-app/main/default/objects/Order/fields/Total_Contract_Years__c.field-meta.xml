<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Total_Contract_Years__c</fullName>
    <description>Custom Field built according to US# BIZ-47145</description>
    <externalId>false</externalId>
    <formula>IF( ISBLANK( SBQQ__Quote__r.First_Segment_Months__c ) ,  (SBQQ__Quote__r.SBQQ__SubscriptionTerm__c)  / 12,  ((SBQQ__Quote__r.SBQQ__SubscriptionTerm__c  -  SBQQ__Quote__r.First_Segment_Months__c) / 12) + 1 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Contract Years</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
