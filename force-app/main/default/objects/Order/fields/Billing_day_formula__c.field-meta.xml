<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_day_formula__c</fullName>
    <description>Returns Billing day for this order</description>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(SBQQ__Quote__r.Override_Next_Billing_date__c),  DAY (DATEVALUE(ActivatedDate)) , DAY(SBQQ__Quote__r.Override_Next_Billing_date__c ))</formula>
    <label>Billing day formula</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
