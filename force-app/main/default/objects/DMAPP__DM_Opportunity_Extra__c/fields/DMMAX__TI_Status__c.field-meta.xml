<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMMAX__TI_Status__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(ISBLANK(DMMAX__TI_Earliest_Scheduled_Review__c) &amp;&amp; ISBLANK(DMMAX__TI_Last_Completed_Scheduled_Review__c), &apos;None&apos;,
IF(ISBLANK(DMMAX__TI_Earliest_Scheduled_Review__c) &amp;&amp; !ISBLANK(DMMAX__TI_Last_Completed_Scheduled_Review__c), &apos;Complete&apos;,
IF(DMMAX__TI_Earliest_Scheduled_Review__c &lt; TODAY(), &apos;Overdue&apos;, &apos;Scheduled&apos;)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Scheduled: T&amp;I schedule date is today or in the future. Overdue: T&amp;I schedule date is in the past. None: No open T&amp;I scheduled.</inlineHelpText>
    <label>Test &amp; Improve Status</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
