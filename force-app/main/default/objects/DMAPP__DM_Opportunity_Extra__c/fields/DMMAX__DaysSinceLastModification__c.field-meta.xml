<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMMAX__DaysSinceLastModification__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>MIN(TODAY() - Datevalue(DMAPP__Opportunity__r.LastModifiedDate), TODAY() - DATEVALUE(LastModifiedDate))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Days since the opportunity was last modified.  
Warning: this may not always be reliable as information *related* to the opportunity may be updated without being reflected here.</inlineHelpText>
    <label>Days Since Last Modification</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
