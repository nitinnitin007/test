<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMMAX__GEN_DaysToClose__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>IF(DMAPP__Opportunity__r.IsClosed,
0, 
DMAPP__Opportunity__r.CloseDate - TODAY()
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Number of days between today and the opportunity close date</inlineHelpText>
    <label>Days To Close</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
