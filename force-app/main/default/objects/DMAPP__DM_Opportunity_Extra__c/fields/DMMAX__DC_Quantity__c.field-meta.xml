<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>DMMAX__DC_Quantity__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <formula>if (DMAPP__Decision_Criteria_Count__c = 0, &apos;None&apos;, 
  if (DMAPP__Decision_Criteria_Count__c &lt;= 3, &apos;Low&apos;, 
    if (DMAPP__Decision_Criteria_Count__c &lt;= 7, &apos;Medium&apos;, &apos;High&apos;)
  )
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Buckets the number of decision criteria into None, Low (1-3), Medium (4-7) or High (8+)</inlineHelpText>
    <label>Decision Criteria Quantity</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
