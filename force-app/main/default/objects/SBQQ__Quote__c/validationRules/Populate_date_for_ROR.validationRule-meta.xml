<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Populate_date_for_ROR</fullName>
    <active>false</active>
    <errorConditionFormula>AND (NOT(ISPICKVAL(ROR__c,&quot;&quot;)),  ISNULL(ROR_Expiration_Date__c))</errorConditionFormula>
    <errorDisplayField>ROR_Expiration_Date__c</errorDisplayField>
    <errorMessage>Please populate ROR Expiration Date</errorMessage>
</ValidationRule>
