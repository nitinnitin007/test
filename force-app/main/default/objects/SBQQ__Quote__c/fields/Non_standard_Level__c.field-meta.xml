<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Non_standard_Level__c</fullName>
    <description>Created for BIZ-47133
To display Non Standard approval reason in LWC</description>
    <externalId>false</externalId>
    <formula>IF((NOT(ISPICKVAL(ROR__c,&apos;&apos;))||(Platform_Conversion__c=true)&amp;&amp; (Development_Obligation__c = false||Non_Standard_Packaging_Structure__c = false)),3,
IF(Development_Obligation__c = true,9,
IF(Non_Standard_Packaging_Structure__c = true, 10, 0)))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Non standard Level</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
