<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Services_Discount_Percentag_Prime__c</fullName>
    <description>Field is created as a part of story: BIZ-49977 to calculate a product as prime discount percentage</description>
    <externalId>false</externalId>
    <formula>IF(
Sixth_Year_Software_Net_Total__c + Fourth_Year_Software_Net_Total__c = 0,
0,
Fourth_Year_Software_Net_Total__c/(Sixth_Year_Software_Net_Total__c + Fourth_Year_Software_Net_Total__c)
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Services Discount Percentage(Prime)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>
