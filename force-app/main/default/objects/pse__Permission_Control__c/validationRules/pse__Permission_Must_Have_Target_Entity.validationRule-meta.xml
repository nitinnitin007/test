<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>pse__Permission_Must_Have_Target_Entity</fullName>
    <active>true</active>
    <description>Permission Control must reference either a Resource or a Region/Practice/Group or Project.</description>
    <errorConditionFormula>AND(OR(ISNULL(pse__Resource__c),(pse__Resource__c=&apos;&apos;)),OR(ISNULL(pse__Region__c),(pse__Region__c=&apos;&apos;)),OR(ISNULL(pse__Practice__c),(pse__Practice__c=&apos;&apos;)),OR(ISNULL(pse__Group__c),(pse__Group__c=&apos;&apos;)),OR(ISNULL(pse__Project__c),(pse__Project__c=&apos;&apos;)))</errorConditionFormula>
    <errorMessage>Permission Control must reference either a Resource or a Region/Practice/Group or Project.</errorMessage>
</ValidationRule>
