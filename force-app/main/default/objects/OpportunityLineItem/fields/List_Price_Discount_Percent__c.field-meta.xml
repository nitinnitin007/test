<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>List_Price_Discount_Percent__c</fullName>
    <externalId>false</externalId>
    <formula>IF(
  OR(
    ISBLANK(OSA_Discount_Amount__c),
    ListPrice = 0
  ),
  0,
  (OSA_Discount_Amount__c-(UnitPrice-ListPrice))/ListPrice
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>List Price Discount %</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <type>Percent</type>
</CustomField>
