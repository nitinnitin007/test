    <description>This process updates the IMF and Upside checkboxes in the background so that FunnelSource can continue to function based on those boxes.</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>IF ([Opportunity].Account.CRD__c &lt;&gt; &quot;&quot;, [Opportunity].Account.CRD__c.Email ,&quot;&quot;) </stringValue>
            </value>
        </processMetadataValues>
        <name>formula_2_myRule_1_A1_0031465532</name>
        <dataType>String</dataType>
        <expression>IF ({!myVariable_current.Account.CRD__c} &lt;&gt; &quot;&quot;, {!myVariable_current.Account.CRD__r.Email} ,&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  [Opportunity].IsClosed = FALSE,
  NOT(ISNULL([Opportunity].AccountId)),
  NOT(ISBLANK([Opportunity].AccountId)),
  NOT(ISBLANK([Opportunity].Account.CRD__c)),
  NOT(ISNULL([Opportunity].Account.CRD__c))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  {!myVariable_current.IsClosed} = FALSE,
  NOT(ISNULL({!myVariable_current.AccountId})),
  NOT(ISBLANK({!myVariable_current.AccountId})),
  NOT(ISBLANK({!myVariable_current.Account.CRD__c})),
  NOT(ISNULL({!myVariable_current.Account.CRD__c}))
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  ISCHANGED([Opportunity].IMF_Upside__c),
  ISPICKVAL([Opportunity].IMF_Upside__c, &quot;In Manager Forecast&quot;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_4</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  ISCHANGED({!myVariable_current.IMF_Upside__c}),
  ISPICKVAL({!myVariable_current.IMF_Upside__c}, &quot;In Manager Forecast&quot;)
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  ISCHANGED([Opportunity].IMF_Upside__c),
  ISPICKVAL([Opportunity].IMF_Upside__c, &quot;Upside&quot;)
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_6</name>
        <dataType>Boolean</dataType>
        <expression>AND(
  ISCHANGED({!myVariable_current.IMF_Upside__c}),
  ISPICKVAL({!myVariable_current.IMF_Upside__c}, &quot;Upside&quot;)
)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
  ISCHANGED([Opportunity].IMF_Upside__c),
  ISBLANK(TEXT([Opportunity].IMF_Upside__c))
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_8</name>
        <dataType>Boolean</dataType>
        <expression>AND(
