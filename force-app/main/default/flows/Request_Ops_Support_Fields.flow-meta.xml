   ISPICKVAL({!Get_Require_Ops_Fields.Contract_Master_Terms__c}, &apos;Trial&apos;),
   ISPICKVAL({!Get_Require_Ops_Fields.Contract_Master_Terms__c}, &apos;New MSSA Signed Separate&apos;)
)</expression>
    </formulas>
    <formulas>
        <name>ContractTerms_CloudAddendum</name>
        <dataType>Boolean</dataType>
        <expression>   AND(
       OR(
