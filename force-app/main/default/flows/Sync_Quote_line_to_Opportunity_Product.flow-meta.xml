    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>
IF (([OpportunityLineItem].Product_Type__c = &quot;Services&quot; || 
[OpportunityLineItem].Product_Type__c = &quot;Software Discount (One Time)&quot; || [OpportunityLineItem].Product_Type__c = &quot;Services Discount&quot; || [OpportunityLineItem].Product_Type__c = &quot;Support&quot; ) , 
[OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__NetPrice__c,
   IF(
AND(
[OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__Quote__c.SBQQ__SubscriptionTerm__c &lt;24,
[OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__Quote__c.SBQQ__SubscriptionTerm__c &gt;12),
[OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__NetPrice__c/[OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__Quote__c.SBQQ__SubscriptionTerm__c,

IF(

  OR(
    ISBLANK([OpportunityLineItem].SBQQ__QuoteLine__c.Segment_Term__c),
    ISNULL([OpportunityLineItem].SBQQ__QuoteLine__c.Segment_Term__c),
    [OpportunityLineItem].SBQQ__QuoteLine__c.Segment_Term__c = 0
  ),
   ([OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__NetPrice__c/12),
   
   


([OpportunityLineItem].SBQQ__QuoteLine__c.SBQQ__NetPrice__c/[OpportunityLineItem].SBQQ__QuoteLine__c.Segment_Term__c)
)))
</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_3_myRule_1_A1_7552507807</name>
        <dataType>Currency</dataType>
        <expression>
IF (({!myVariable_current.Product_Type__c} = &quot;Services&quot; || 
{!myVariable_current.Product_Type__c} = &quot;Software Discount (One Time)&quot; || {!myVariable_current.Product_Type__c} = &quot;Services Discount&quot; || {!myVariable_current.Product_Type__c} = &quot;Support&quot; ) , 
{!myVariable_current.SBQQ__QuoteLine__r.SBQQ__NetPrice__c},
   IF(
AND(
{!myVariable_current.SBQQ__QuoteLine__r.SBQQ__Quote__r.SBQQ__SubscriptionTerm__c} &lt;24,
{!myVariable_current.SBQQ__QuoteLine__r.SBQQ__Quote__r.SBQQ__SubscriptionTerm__c} &gt;12),
{!myVariable_current.SBQQ__QuoteLine__r.SBQQ__NetPrice__c}/{!myVariable_current.SBQQ__QuoteLine__r.SBQQ__Quote__r.SBQQ__SubscriptionTerm__c},

IF(

  OR(
    ISBLANK({!myVariable_current.SBQQ__QuoteLine__r.Segment_Term__c}),
    ISNULL({!myVariable_current.SBQQ__QuoteLine__r.Segment_Term__c}),
    {!myVariable_current.SBQQ__QuoteLine__r.Segment_Term__c} = 0
  ),
   ({!myVariable_current.SBQQ__QuoteLine__r.SBQQ__NetPrice__c}/12),
   
   


({!myVariable_current.SBQQ__QuoteLine__r.SBQQ__NetPrice__c}/{!myVariable_current.SBQQ__QuoteLine__r.Segment_Term__c})
)))</expression>
        <scale>18</scale>
    </formulas>
